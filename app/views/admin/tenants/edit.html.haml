
%h1 Edit Existing Tenant

= form_tag admin_tenant_path(@tenant), :method => :put do

	= label :tenant, :title, 'Title'
	= text_field :tenant, 'title'
	
	= label :tenant, :description, 'Description'
	= text_field :tenant, 'description'	

	= label :tenant, :logo, 'Logo'
	= text_field :tenant, 'logo'

	= submit_tag 'Update Tenant Info'
